				<question id='bene_middle_name' text='Middle' labelWidth="50px"
					type='TEXT' answerWidth="50px;" />
				<question id='bene_last_name' text='Last Name' labelWidth="80px"
					required="true" type='TEXT' answerWidth="140px;" />
			</group>
			<group labelWidth="250px">
				<question id='bene_relation' text='Relationship to the Insured'
					required="true" type='LISTBOX' lookup="glic:RELATIONSHIP_TYPE"
					answerSet="" />
			</group>
		</section>
		<section title="Primary Beneficiary" repeating="true">
			<group associated="true">
				<question id='bene_first_name2' text='Primary Beneficiary 2: First Name'
