	<page title="Part 3B - Medical Questions of Proposed Insured">
		<section>
			<group labelWidth="500px">
				<question id='weight_chg_ind'
					text='Has your weight changed by more than 10 pounds in the past year?'
					required="true" type='RADIO' answerSet='Y|N' />
			</group>
		</section>
		<section title="Significant Weight Change">
			<displayWhen>#{EFORM.weight_chg_ind=="Y"}</displayWhen>
			<group labelWidth="200px">
				<question id='weight_chg_pounds' text='(Weight change in pounds)'
					type='TEXT' validation='number' required="true" />
				<question id='weight_gain_loss' text='Was this change a gain or a loss?'
					type='RADIO' answerSet='Gain|Loss' required="true" />
				<question id='weightchg_reasons' text='Please specify'
					type='LISTBOX' lookup="merica:WEIGHTCHG_REASON" answerSet=":Please select a reason"
					labelWidth="60px" required="true" />
			</group>
		</section>
		<section title="Medical Weight Change">
			<displayWhen>#{EFORM.weight_chg_ind=="Y" and EFORM.weightchg_reasons=="MEDICAL_WC"}</displayWhen>
			<group labelWidth="400px">
				<question id='medicalwccompleter' text='Please specify'
					type='TEXT' answerWidth="350px;" required="true"
					autocompleteLookup="merica:diagnosis" />
			</group>
			<group parentQuestionId="medicalwccompleter"
				dynamicQuestionContext="merica:diagnosis" labelWidth="400px">
			</group>
		</section>
		<section title="Other Weight Change">
			<displayWhen>#{EFORM.weightchg_reasons=="OTHER_WC"}</displayWhen>
